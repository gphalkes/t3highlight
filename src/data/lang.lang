format = 1

%include = "def/generic.lang"
%highlight { use = "hash-comment" }

%highlight {
	regex = '(?<!\w)(?:%include|%highlight|%define|start|end|regex|use|style|' +
		'delim-style|nested|true|false|yes|no|format|name)'
	style = "keyword"
}
%highlight {
	start = '"'
	%highlight {
		regex = '""'
	}
	end = '"|$'
	style = "string"
}
%highlight {
	start = "'"
	%highlight {
		regex = "''"
	}
	end = "'|$"
	style = "string"
}
